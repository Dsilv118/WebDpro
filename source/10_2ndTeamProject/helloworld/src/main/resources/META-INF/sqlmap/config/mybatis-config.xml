<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/> <!-- date에 널 허용 -->
        <setting name="cacheEnabled" value="false" />
	</settings>
	<typeAliases>
		<typeAlias type="com.lec.helloworld.vo.Member" alias="Member"/>
		<typeAlias type="com.lec.helloworld.vo.QnaBoard" alias="QnaBoard"/>
		<typeAlias type="com.lec.helloworld.vo.ThRev" alias="ThRev"/>
		<typeAlias type="com.lec.helloworld.vo.ThRes" alias="ThRes"/>
		<typeAlias type="com.lec.helloworld.vo.Theater" alias="Theater"/>
		<typeAlias type="com.lec.helloworld.vo.Notice" alias="Notice" />
		<typeAlias type="com.lec.helloworld.vo.Admin" alias="Admin"/>
		<typeAlias type="com.lec.helloworld.vo.Attraction" alias="Attraction"/>
		<typeAlias type="com.lec.helloworld.vo.Ticket" alias="Ticket"/>
		<typeAlias type="com.lec.helloworld.vo.TicketRes" alias="TicketRes"/>
		<typeAlias type="com.lec.helloworld.vo.TicketResDetail" alias="TicketResDetail"/>
		<typeAlias type="com.lec.helloworld.vo.At_Review" alias="At_Review"/>
	</typeAliases>
	<mappers>
		<mapper resource="META-INF/sqlmap/mappers/member.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/qnaBoard.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/thRev.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/thRes.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/theater.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/notice.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/admin.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/attraction.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/ticket.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/ticketRes.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/ticketResDetail.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/at_Review.xml"/>
	</mappers>
</configuration>
